from rest_framework import serializers
from seedsource.models import SeedZone
from typing import Any

class RunConfigurationSerializer(serializers.ModelSerializer):
    configuration: Any = ...
    class Meta:
        model: Any = ...
        fields: Any = ...
        read_only_fields: Any = ...

class SeedZoneSerializer(serializers.ModelSerializer):
    elevation_at_point: Any = ...
    elevation_band: Any = ...
    _elevation: Any = ...
    class Meta:
        model: Any = ...
        fields: Any = ...
    @property
    def _elevation_at_point(self): ...
    def get_elevation_at_point(self, obj: Any): ...
    def get_elevation_band(self, obj: SeedZone) -> Any: ...

class TransferLimitSerializer(serializers.ModelSerializer):
    zone: Any = ...
    class Meta:
        model: Any = ...
        fields: Any = ...

class GenerateReportSerializer(serializers.Serializer):
    configuration: Any = ...
    tile_layers: Any = ...
    zoom: Any = ...
    opacity: Any = ...

class RegionSerializer(serializers.ModelSerializer):
    class Meta:
        model: Any = ...
        fields: Any = ...
