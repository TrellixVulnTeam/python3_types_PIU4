import requests
from typing import Any, Dict, List, Optional, Tuple, Union

class Caller:
    headers: Any = ...
    logger: Any = ...
    host: str = ...
    user: str = ...
    password: str = ...
    use_https: bool = ...
    port: int = ...
    url_prefix: str = ...
    validate_cert: bool = ...
    timeout: int = ...
    retries: int = ...
    def __init__(self) -> None: ...
    @property
    def baseurl(self) -> str: ...
    @property
    def http_auth(self) -> Tuple[str, str]: ...
    def delete(self, url: str, parameters: Optional[Dict]=...) -> requests.Response: ...
    def get(self, url: str, parameters: Optional[Dict]=...) -> requests.Response: ...
    def post(self, url: str, data: Union[None, Dict, List]=...) -> requests.Response: ...
    def test_connection(self) -> bool: ...
