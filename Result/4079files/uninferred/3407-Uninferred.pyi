from django.shortcuts import get_object_or_404 as get_object_or_404
from django.views import View
from django.views.generic import CreateView, ListView, TemplateView, UpdateView
from polygon.problem2.views.base import ProblemRevisionMixin
from typing import Any, Optional

class TemplateList(ProblemRevisionMixin, ListView):
    template_name: str = ...
    context_object_name: str = ...
    polygon_title: str = ...
    def get_queryset(self): ...

class RevisionTemplateMixin(ProblemRevisionMixin):
    model_class: Any = ...
    template: Any = ...
    def init_revision(self, *args: Any, **kwargs: Any) -> None: ...

class TemplateCreateView(ProblemRevisionMixin, CreateView):
    form_class: Any = ...
    template_name: str = ...
    polygon_title: str = ...
    def get_success_url(self): ...
    object: Any = ...
    def form_valid(self, form: Any): ...

class TemplateUpdateView(RevisionTemplateMixin, UpdateView):
    form_class: Any = ...
    template_name: str = ...
    polygon_title: str = ...
    def get_success_url(self): ...
    def get_object(self, queryset: Optional[Any] = ...): ...
    object: Any = ...
    def form_valid(self, form: Any): ...

class TemplateDeleteView(ProblemRevisionMixin, View):
    def post(self, request: Any, *args: Any, **kwargs: Any): ...

class TemplatePreview(RevisionTemplateMixin, TemplateView):
    template_name: str = ...
    polygon_title: str = ...
    def get_context_data(self, **kwargs: Any): ...
