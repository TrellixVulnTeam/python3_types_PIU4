from bson import ObjectId
from flask_mongoengine import Document
from typing import Any

class WorkSession(Document):
    user: Any = ...
    task: Any = ...
    task_type: Any = ...
    answer: Any = ...
    start_time: Any = ...
    end_time: Any = ...
    activity: Any = ...
    meta: Any = ...
    @classmethod
    def get_total_user_time_precise(cls: Any, user_id: ObjectId) -> int: ...
    @classmethod
    def get_total_user_time_approximate(cls: Any, user_id: ObjectId) -> int: ...
