from threading import Thread
from typing import Any

def progress(i: int) -> bytes: ...

class Messenger(Thread):
    __armor: Any = ...
    __health: Any = ...
    __kills: Any = ...
    __money: Any = ...
    __refresh: bool = ...
    __start: bool = ...
    __status: str = ...
    ser_arduino: Any = ...
    def __init__(self, ser_arduino: Any) -> None: ...
    @property
    def armor(self) -> int: ...
    @armor.setter
    def armor(self, armor: int) -> None: ...
    @property
    def money(self) -> int: ...
    @money.setter
    def money(self, money: int) -> None: ...
    @property
    def health(self) -> int: ...
    @health.setter
    def health(self, health: int) -> None: ...
    @property
    def status(self) -> str: ...
    @status.setter
    def status(self, status: str) -> None: ...
    @property
    def kills(self) -> tuple: ...
    @kills.setter
    def kills(self, kills_heads: tuple) -> None: ...
    def run(self) -> None: ...
    def bomb_timer(self) -> None: ...
    def write_player_stats(self) -> None: ...
    def idle(self) -> None: ...
    def shutdown(self) -> None: ...
