from typing import Any, Optional

class Error(Exception):
    level: Any = ...
    http_code: Any = ...
    message: Any = ...
    code: Any = ...
    def __init__(self, message: Optional[Any] = ..., *, code: Optional[Any] = ..., level: Optional[Any] = ..., http_code: Optional[Any] = ...) -> None: ...
    def __str__(self): ...
    def to_dict(self): ...

class ServerError(Error):
    level: Any = ...
    http_code: int = ...

class ClientError(Error):
    level: Any = ...
    http_code: int = ...

def handle_error(error: Any, exception: Optional[Any] = ...): ...
