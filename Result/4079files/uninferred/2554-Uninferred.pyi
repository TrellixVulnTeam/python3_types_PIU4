import typing
from typing import Any

class ScratchDirError(Exception): ...
class ScratchDirInactiveError(ScratchDirError): ...

class ScratchDir:
    prefix: Any = ...
    suffix: Any = ...
    base: Any = ...
    root: Any = ...
    wd: Any = ...
    def __init__(self, prefix: str=..., suffix: str=..., base: typing.Optional[str]=..., root: typing.Optional[str]=..., wd: typing.Optional[str]=...) -> None: ...
    def __enter__(self) -> ScratchDir: ...
    def __exit__(self, exc_type: Any, exc_val: Any, exc_tb: Any) -> None: ...
    @property
    def is_active(self) -> bool: ...
    def setup(self) -> None: ...
    def teardown(self) -> None: ...
    def child(self, prefix: str=..., suffix: str=...) -> ScratchDir: ...
    def file(self, mode: str=..., buffering: int=..., encoding: typing.Optional[str]=..., newline: typing.Optional[str]=..., suffix: typing.Optional[str]=..., prefix: typing.Optional[str]=..., dir: typing.Optional[str]=...) -> typing.IO: ...
    def named(self, mode: str=..., buffering: int=..., encoding: typing.Optional[str]=..., newline: typing.Optional[str]=..., suffix: typing.Optional[str]=..., prefix: typing.Optional[str]=..., dir: typing.Optional[str]=..., delete: bool=...) -> typing.IO: ...
    def spooled(self, max_size: int=..., mode: str=..., buffering: int=..., encoding: typing.Optional[str]=..., newline: typing.Optional[str]=..., suffix: typing.Optional[str]=..., prefix: typing.Optional[str]=..., dir: typing.Optional[str]=...) -> typing.IO: ...
    def secure(self, suffix: typing.Optional[str]=..., prefix: typing.Optional[str]=..., dir: typing.Optional[str]=..., text: bool=..., return_fd: bool=...) -> typing.Union[str, typing.Tuple[int, str]]: ...
    def directory(self, suffix: typing.Optional[str]=..., prefix: typing.Optional[str]=..., dir: bool=...) -> str: ...
    def filename(self, suffix: typing.Optional[str]=..., prefix: typing.Optional[str]=...) -> str: ...
    def join(self, *paths: str) -> str: ...
    TemporaryFile: Any = ...
    NamedTemporaryFile: Any = ...
    SpooledTemporaryFile: Any = ...
    mkstemp: Any = ...
    mkdtemp: Any = ...
