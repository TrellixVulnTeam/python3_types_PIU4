from setuptools.command.test import test as test_command
from typing import Any

class PyTest(test_command):
    user_options: Any = ...
    pytest_args: Any = ...
    def initialize_options(self) -> None: ...
    test_args: Any = ...
    test_suite: bool = ...
    def finalize_options(self) -> None: ...
    def run_tests(self) -> None: ...
