from typing import Any

class AppDispatcher:
    def exit_clicked(self) -> None: ...

class PlayerDispatcher:
    def play_clicked(self) -> None: ...
    def pause_clicked(self) -> None: ...
    def change_station_clicked(self, station: Any) -> None: ...
    def playing_started(self) -> None: ...
    def playing_paused(self) -> None: ...
    def song_changed(self, title: Any) -> None: ...
    def station_changed(self, station: Any) -> None: ...
    def playing_state_changed(self, new_state: Any) -> None: ...

class RecorderDispatcher:
    def start_record_clicked(self, title: Any) -> None: ...
    def stop_record_clicked(self) -> None: ...
    def recording_started(self, title: Any) -> None: ...
    def recording_stopped(self) -> None: ...
    def recording_state_changed(self, new_state: Any) -> None: ...

class StorageDispatcher:
    def add_to_favourites_clicked(self, title: Any) -> None: ...
    def blacklist_song_clicked(self, title: Any) -> None: ...
    def manage_favourites_clicked(self) -> None: ...
    def manage_blacklist_clicked(self) -> None: ...

app: Any
player: Any
recorder: Any
storage: Any
