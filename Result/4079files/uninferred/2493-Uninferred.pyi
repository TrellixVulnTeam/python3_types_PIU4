from enum import Enum
from kivy.uix.image import Image
from typing import Any, Optional

class SlideMode(Enum):
    NORMAL: int = ...
    FIT_SCREEN: int = ...
    FIT_WIDTH: int = ...
    FIT_HEIGHT: int = ...

class Slide(Image):
    mode: Any = ...
    load_mode: Any = ...
    is_loaded: bool = ...
    texture: Any = ...
    def __init__(self, session: Optional[Any] = ..., load_mode: Any = ..., cursor: Optional[Any] = ..., **kwargs: Any) -> None: ...
    size: Any = ...
    def on_texture_size(self, instance: Any, value: Any) -> None: ...
    def on_image_loaded(self, *args: Any) -> None: ...
    size_hint: Any = ...
    def on_mode(self, obj: Any, value: Any) -> None: ...
    def reset_zoom(self) -> None: ...
    height: Any = ...
    def on_width(self, obj: Any, value: Any) -> None: ...
    width: Any = ...
    def on_height(self, obj: Any, value: Any) -> None: ...
    @property
    def zoom(self): ...
    @zoom.setter
    def zoom(self, value: Any) -> None: ...
