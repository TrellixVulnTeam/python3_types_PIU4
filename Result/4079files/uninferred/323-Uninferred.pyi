from requests.auth import AuthBase
from typing import Any, Optional

class TokenAuthentication(AuthBase):
    token: Any = ...
    scheme: Any = ...
    def __init__(self, token: Any, scheme: str = ...) -> None: ...
    def __call__(self, request: Any): ...

class SessionAuthentication(AuthBase):
    safe_methods: Any = ...
    csrf_cookie_name: Any = ...
    csrf_header_name: Any = ...
    csrf_token: Any = ...
    def __init__(self, csrf_cookie_name: Optional[Any] = ..., csrf_header_name: Optional[Any] = ...) -> None: ...
    def store_csrf_token(self, response: Any, **kwargs: Any) -> None: ...
    def __call__(self, request: Any): ...
