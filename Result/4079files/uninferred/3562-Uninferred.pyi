from ..core import Provider, ProviderResource, Response as Response
from typing import Any

class StatuspageMixin:
    base_url: str = ...
    name: str = ...
    path_to_errors: Any = ...
    site_url: str = ...

class StatuspageComponents(StatuspageMixin, ProviderResource):
    resource_name: str = ...
    components_url: str = ...
    _required: Any = ...
    _schema: Any = ...
    def _get_resource(self, data: dict) -> dict: ...

class Statuspage(StatuspageMixin, Provider):
    incidents_url: str = ...
    _resources: Any = ...
    realtime_statuses: Any = ...
    scheduled_statuses: Any = ...
    __component_ids: Any = ...
    _required: Any = ...
    _schema: Any = ...
    def _validate_data_dependencies(self, data: dict) -> dict: ...
    def _prepare_data(self, data: dict) -> dict: ...
    def _send_notification(self, data: dict) -> Response: ...
