from typing import Any, Optional

class MyClassMixin:
    @property
    def any(self): ...
    _any: Any = ...
    @any.setter
    def any(self, value: Any) -> None: ...
    def __init__(self, *, any: Optional[Any] = ..., **kwargs: Any) -> None: ...
    do_it: Any = ...

class MyOtherClassMixin:
    @property
    def other(self): ...
    _other: Any = ...
    @other.setter
    def other(self, value: Any) -> None: ...
    mock_other: Any = ...
    def __init__(self, *, other: Optional[Any] = ..., **kwargs: Any) -> None: ...
