from typing import Any, List, NamedTuple

def check_type(obj: Any, candidate_type: Any, reltype: str=...) -> bool: ...
def check_args(func: Any): ...

class IssueDescription(NamedTuple):
    name: str
    expected_type: Any
    value: Any
    missing_parameter: bool
    generic_message: str = ...
    def __repr__(self) -> str: ...

class DetailedTypeError(TypeError):
    issues: Any = ...
    def __init__(self, issues: List[IssueDescription]) -> None: ...
    def __str__(self): ...
    def __iter__(self) -> Any: ...
    def __len__(self): ...
