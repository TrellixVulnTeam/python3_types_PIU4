from hvapi.clr.base import ManagementObject
from typing import Any, Optional

class MOWrapper(ManagementObject):
    Path: Any = ...
    parent: Any = ...
    def __init__(self, mo: ManagementObject, parent: MOWrapper=...) -> None: ...

class JobWrapper(MOWrapper):
    MO_CLS: Any = ...
    def wait(self) -> None: ...

class VirtualSystemManagementService(MOWrapper):
    MO_CLS: str = ...
    def SetGuestNetworkAdapterConfiguration(self, ComputerSystem: Any, *args: Any): ...
    def ModifyResourceSettings(self, *args: Any): ...
    def ModifySystemSettings(self, SystemSettings: Any): ...
    def AddResourceSettings(self, AffectedConfiguration: Any, *args: Any): ...
    def DefineSystem(self, SystemSettings: Any, ResourceSettings: Any = ..., ReferenceConfiguration: Optional[Any] = ...): ...
