from django.db import models
from typing import Any

class BlogQuerySet(models.QuerySet):
    def with_likes(self): ...
    def with_dislikes(self): ...
    def with_likes_flag(self, user: Any): ...

class BlogRevision(models.Model):
    title: Any = ...
    text: Any = ...
    author: Any = ...
    create_time: Any = ...
    class Meta:
        ordering: Any = ...

class Blog(models.Model):
    title: Any = ...
    text: Any = ...
    author: Any = ...
    visible: Any = ...
    create_time: Any = ...
    edit_time: Any = ...
    likes: Any = ...
    recommend: Any = ...
    revisions: Any = ...
    hide_revisions: Any = ...
    objects: Any = ...
    class Meta:
        ordering: Any = ...

class BlogLikes(models.Model):
    BLOG_LIKE_FLAGS: Any = ...
    blog: Any = ...
    user: Any = ...
    flag: Any = ...
    class Meta:
        unique_together: Any = ...

class Comment(models.Model):
    text: Any = ...
    author: Any = ...
    create_time: Any = ...
    blog: Any = ...
    problem: Any = ...
    class Meta:
        ordering: Any = ...
