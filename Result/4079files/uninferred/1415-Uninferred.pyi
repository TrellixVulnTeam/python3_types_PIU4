from gi.repository import GObject
from typing import Any

class Stream(GObject.GObject):
    def get_is_active(self): ...
    _is_active: Any = ...
    def set_is_active(self, value: bool) -> Any: ...
    is_active: Any = ...
    index: Any = ...
    name: Any = ...
    display_name: Any = ...
    description: Any = ...
    ports: Any = ...
    card_index: Any = ...
    def __init__(self, index: int, name: str, display_name: str, description: str, ports: dict, card_index: int) -> None: ...
    def activate_port_by_name(self, name: str) -> Any: ...
    def get_display_name(self) -> str: ...

class Source(Stream): ...
class Sink(Stream): ...
