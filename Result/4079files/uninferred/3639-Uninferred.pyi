from settings import *
from abc import ABCMeta
from copy import deepcopy as deepcopy
from typing import Any, Dict, List, Union

class DataFileMixin(metaclass=ABCMeta):
    @staticmethod
    def _get_by_ID(ID: int, obj_type: str, file: str, file_format: str=...) -> Dict: ...
    def get_item_by_ID(self, ID: int, file: str=...) -> Dict: ...
    def get_enemy_by_ID(self, ID: int, file: str=...) -> Dict: ...
    def get_npc_by_ID(self, ID: int, file: str=...) -> Dict: ...
    def get_entity_by_ID(self, ID: int, file: str=...) -> Dict: ...

class ReprMixin(metaclass=ABCMeta):
    def __repr__(self): ...

class LevelMixin(metaclass=ABCMeta):
    level: Any = ...
    experience: Any = ...
    exponent: Any = ...
    _base_exp: Any = ...
    max_level: Any = ...
    def __init__(self, **kwargs: Any) -> None: ...
    @property
    def next_level(self) -> int: ...
    def level_up(self, print_exp: bool=...) -> Union[str, None]: ...
    def give_exp(self, amount: int, check_level_up: Any=..., print_exp: Any=...) -> Union[str, None]: ...

class SpritesMixin(metaclass=ABCMeta):
    @staticmethod
    def __load_sprites(type: str, obj: str) -> List: ...
    def load_char_sprites(self, name: str) -> Any: ...
    def load_item_sprites(self, ID: int) -> Any: ...
