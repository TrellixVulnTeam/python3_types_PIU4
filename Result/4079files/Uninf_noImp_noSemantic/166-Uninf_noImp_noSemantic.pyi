from typing import Any

LEVEL_COLORS: Any
DEFAULT_FORMAT: str

class ComposableFormatter:
    _parent_formatter: Any = ...
    def __init__(self, *args: Any, **kwargs: Any) -> None: ...
    def new_formatter(self, *args: Any, **kwargs: Any): ...
    def __getattr__(self, name: Any): ...

class ColorFormatter(ComposableFormatter):
    _use_color: Any = ...
    def __init__(self, *args: Any, **kwargs: Any) -> None: ...
    def reformat(self, fmt: Any): ...
    @staticmethod
    def remove_color(fmt: Any): ...
    def new_formatter(self, fmt: Any, *args: Any, **kwargs: Any): ...
    def format(self, *args: Any, **kwargs: Any): ...
