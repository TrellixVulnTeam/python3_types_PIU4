from core.models import Entity, Team
from django.db import models
from typing import Any

class Token(models.Model):
    LENGTH: int = ...
    CONSONANTS: str = ...
    VOWELS: str = ...
    code: Any = ...
    entity: Any = ...
    used_by: Any = ...
    used_time: Any = ...
    value: Any = ...
    def _use_commit(self, team: Any) -> None: ...
    def use(self, team: Team) -> Any: ...
    @staticmethod
    def get(code: str) -> Any: ...
    @staticmethod
    def randomcode(): ...
    @staticmethod
    def generate_one(entity: Entity, amount: Any=...) -> Any: ...
    def __repr__(self): ...

class TokenUnusableException(Exception): ...
