import enum

class Command(enum.IntEnum):
    SYNC: int = ...
    CNXN: int = ...
    AUTH: int = ...
    OPEN: int = ...
    OKAY: int = ...
    CLSE: int = ...
    WRTE: int = ...

class CommandResponse(enum.Enum):
    OKAY: str = ...
    FAIL: str = ...
    def __str__(self): ...

class AuthType(enum.IntEnum):
    TOKEN: int = ...
    SIGNATURE: int = ...
    RSAPUBLICKEY: int = ...

class SystemType(enum.Enum):
    BOOTLOADER: str = ...
    DEVICE: str = ...
    HOST: str = ...
    def __str__(self): ...
