from typing import Any, Dict, List, Optional, SupportsInt, Union

def require_str(value: Any, convert: bool=..., allow_none: bool=...) -> Any: ...
def require_bool(value: Optional[Union[bool, str, int]], convert: bool=..., allow_none: bool=...) -> Any: ...
def require_dict(value: Optional[Dict[Any, Any]], key_type: Any=..., value_type: Any=..., allow_none: bool=...) -> Any: ...
def require_list(value: Optional[List[Any]], item_type: Any=..., allow_none: bool=...) -> Any: ...
def require_int(value: Optional[Union[SupportsInt, str, bytes]], allow_none: bool=...) -> Any: ...
