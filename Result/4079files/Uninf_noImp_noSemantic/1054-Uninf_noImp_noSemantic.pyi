from typing import Any

class JudgeContext:
    __problem_id: Any = ...
    __source_code: Any = ...
    __programming_language: Any = ...
    def __init__(self, problem_id: str=..., source_code: str=..., programming_language: str=...) -> None: ...
    @property
    def problem_id(self) -> str: ...
    @problem_id.setter
    def problem_id(self, value: str) -> None: ...
    @property
    def source_code(self) -> str: ...
    @source_code.setter
    def source_code(self, value: str) -> None: ...
    @property
    def programming_language(self) -> str: ...
    @programming_language.setter
    def programming_language(self, value: str) -> Any: ...
