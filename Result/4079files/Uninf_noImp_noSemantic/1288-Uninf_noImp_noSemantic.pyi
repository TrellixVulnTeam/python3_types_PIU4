from typing import Any

class AxesHandler:
    def reset_attempts(self, *, ip_address: str=..., username: str=...) -> int: ...
    def reset_logs(self, *, age_days: int=...) -> int: ...
    def is_allowed(self, request: Any, credentials: dict=...) -> bool: ...
    def user_login_failed(self, sender: Any, credentials: dict, request: Any=..., **kwargs: Any) -> Any: ...
    def user_logged_in(self, sender: Any, request: Any, user: Any, **kwargs: Any) -> None: ...
    def user_logged_out(self, sender: Any, request: Any, user: Any, **kwargs: Any) -> None: ...
    def post_save_access_attempt(self, instance: Any, **kwargs: Any) -> None: ...
    def post_delete_access_attempt(self, instance: Any, **kwargs: Any) -> None: ...
    def is_blacklisted(self, request: Any, credentials: dict=...) -> bool: ...
    def is_whitelisted(self, request: Any, credentials: dict=...) -> bool: ...
    def is_locked(self, request: Any, credentials: dict=...) -> bool: ...
    def get_failures(self, request: Any, credentials: dict=...) -> int: ...
    def is_admin_site(self, request: Any) -> bool: ...
