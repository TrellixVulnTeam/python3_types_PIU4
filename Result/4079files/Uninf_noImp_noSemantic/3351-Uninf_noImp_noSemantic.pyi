from typing import Any, Optional

class PtyTimeoutError(Exception): ...

class InteractivePTY:
    _tool: Any = ...
    _tool_args: Any = ...
    _prompt: Any = ...
    _encoding: Any = ...
    _exit_cmd: Any = ...
    _raw_buffer: bytes = ...
    proc: Any = ...
    proc_rc: Any = ...
    sel: Any = ...
    def __init__(self, tool: Any, prompt: Any, exit_cmd: Any, args: Any = ..., encoding: str = ...) -> None: ...
    def __del__(self) -> None: ...
    def close(self) -> None: ...
    def get_data(self, marker: str = ..., timeout: int = ...): ...
    @staticmethod
    def _parse_marker(marker: Any, data: Any): ...
    def send_data(self, data: Any, add_crlf: bool = ...): ...
    def first_prompt(self): ...
    def sync_prompt(self): ...
    def cmd(self, cmd: Any, marker: Optional[Any] = ..., as_string: bool = ..., timeout: int = ..., trim_cmd: bool = ..., add_crlf: bool = ...): ...
    def run_script(self, script: Any): ...
