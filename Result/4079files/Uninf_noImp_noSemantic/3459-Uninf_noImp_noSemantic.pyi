from enum import Enum
from typing import Any

class Color(Enum):
    RED: str = ...
    BLUE: str = ...
    GREEN: str = ...
    ORANGE: str = ...
    PURPLE: str = ...
    TURQUOISE: str = ...
    WHITE: str = ...
    BLACK: str = ...
    SQUARE: str = ...
    def FromRGB(r: Any, g: Any, b: Any): ...
    def Mix(color1: Any, color2: Any, ratio: float = ...): ...
    def Lighter(color: Any, whiteRatio: float = ...): ...
    def Darker(color: Any, blackRatio: float = ...): ...
