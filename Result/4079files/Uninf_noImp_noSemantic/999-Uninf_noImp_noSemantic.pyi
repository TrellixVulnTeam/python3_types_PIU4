from tqdm import tqdm as tqdm
from typing import Any

def print_json_patch(json: Any) -> None: ...

class Output:
    porcelain: bool = ...
    progress_bars: Any = ...
    silent: bool = ...
    verbosity: int = ...
    @staticmethod
    def initialize(porcelain: bool = ..., silent: bool = ..., verbosity: int = ...) -> None: ...
    @staticmethod
    def message(message: Any, *args: Any) -> None: ...
    @staticmethod
    def exception(exception: Any) -> None: ...
    @staticmethod
    def add_progress_bar(id: Any, desc: Any, unit: str = ...) -> None: ...
    @staticmethod
    def update_progress_bar(id: Any, remaining: float = ...) -> None: ...
