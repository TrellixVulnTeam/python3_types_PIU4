from typing import Any, Optional

class OAuth:
    AUTHORIZE_URL: str = ...
    TOKEN_URL: str = ...
    client_id: Any = ...
    client_secret: Any = ...
    redirect_uri: Any = ...
    scopes: Any = ...
    state: Any = ...
    _token: Any = ...
    auto_refresh: Any = ...
    session: Any = ...
    def __init__(self, client_id: Any, client_secret: Any, redirect_uri: Optional[Any] = ..., scopes: Any = ..., state: Optional[Any] = ..., auto_refresh: Optional[Any] = ..., requests: Any = ...) -> None: ...
    @property
    def authorize_url(self): ...
    @property
    def token(self): ...
    @token.setter
    def token(self, value: Any) -> None: ...
    def parse_response_code(self, url: Any): ...
    def request_token(self, url_or_code: Any) -> None: ...
    def request_client_credentials(self) -> None: ...
    def refresh_token(self) -> None: ...
    def refresh_token_if_needed(self, expires_in: int = ...) -> None: ...
