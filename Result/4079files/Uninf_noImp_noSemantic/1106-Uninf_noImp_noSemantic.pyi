from gi.repository import GObject
from typing import Any

class CardProfile(GObject.GObject):
    name: Any = ...
    description: Any = ...
    num_sinks: Any = ...
    num_sources: Any = ...
    priority: Any = ...
    is_active: Any = ...
    def __init__(self, name: str, description: str, num_sinks: int, num_sources: int, priority: int, is_active: bool) -> None: ...

class Card(GObject.GObject):
    @staticmethod
    def find_stream_port(card_port: Any, sources: dict, sinks: dict) -> Any: ...
    index: Any = ...
    name: Any = ...
    display_name: Any = ...
    driver: Any = ...
    profiles: Any = ...
    ports: Any = ...
    proplist: Any = ...
    description: Any = ...
    def __init__(self, index: int, name: str, display_name: str, driver: str, profiles: dict, ports: dict, proplist: Any) -> None: ...
    def get_property_str(self, name: str) -> str: ...
    def get_display_name(self) -> str: ...
    def get_descriptive_name(self) -> str: ...
    def update_port_activity(self, sources: dict, sinks: dict) -> Any: ...
