from abc import ABCMeta, abstractmethod
from typing import Any

logger: Any

class BaseUpdater(metaclass=ABCMeta):
    _useragent: Any = ...
    _externalIp: Any = ...
    @abstractmethod
    def build_payload(self): ...
    @abstractmethod
    def parse_result(self, response: Any): ...
    @abstractmethod
    def updater(self): ...
    @classmethod
    def get_external_ip(self) -> None: ...

class BaseHTTPUpdater(BaseUpdater):
    _api_url: Any = ...
    _query: Any = ...
    _data: Any = ...
    _headers: Any = ...
    def __int__(self, *args: Any, **kwargs: Any) -> None: ...
    @abstractmethod
    def build_payload(self): ...
    def updater(self): ...

class Base3FactsHTTPUpdater(BaseHTTPUpdater):
    _username: Any = ...
    _password: Any = ...
    _hostname: Any = ...
    def __init__(self, hostname: Any, username: Any, password: Any, *args: Any, **kwargs: Any) -> None: ...
    def build_payload(self): ...
