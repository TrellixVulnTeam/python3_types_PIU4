from typing import Any, Optional

__version__: str
DEFAULT_CONFIG: Any
DEFAULT_TENANT: Any
DEFAULT_ENV: Any

class AppFlow:
    def update(self, branch: str = ...) -> None: ...
    def init(self, tenant: Optional[Any] = ..., env: Optional[Any] = ...) -> None: ...
    def ssh(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def vhosts(self, tenant: Any = ...) -> None: ...
    def reset(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def status(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def checkout(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def checkin(self, tenant: Any = ..., env: Any = ..., commit: str = ...) -> None: ...
    def decrypt(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def encrypt(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def tags(self, tenant: Any = ..., env: Any = ...) -> None: ...
    def provision(self, tenant: Any=..., env: Any=..., limit: str=..., tags: str=..., skip_tags: str=..., firstrun: bool=..., local: bool=..., debug: bool=...) -> Any: ...
    def get(self, file: Any, key: Optional[Any] = ...) -> None: ...
    def set(self, file: Any, key: Any, value: Any) -> None: ...
    def rm(self, file: Any, key: Any) -> None: ...
    def add(self, file: Any, key: Any, value: Any) -> None: ...
    def version(self) -> None: ...
