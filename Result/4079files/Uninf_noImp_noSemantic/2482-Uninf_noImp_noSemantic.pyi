    def add_feature(self, feature: ChangelogItem) -> Any: ...
    def add_fix(self, fix: ChangelogItem) -> Any: ...
    def add_internal(self, internal: ChangelogItem) -> Any: ...

def collect_changelog(post_number: int, login_or_token: str, password: str=...) -> Any: ...
def main() -> None: ...
