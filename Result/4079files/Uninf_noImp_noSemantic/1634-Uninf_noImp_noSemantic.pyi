from keras.callbacks import Callback
from typing import Any, Optional

class NotSoChattyLogger(Callback):
    _print_every: Any = ...
    _partial_counter: int = ...
    _total_counter: Any = ...
    _start: Any = ...
    _metrics: Any = ...
    _epoch: int = ...
    def __init__(self, print_every: int = ..., start: Optional[Any] = ..., counter: Optional[Any] = ..., metrics: Optional[Any] = ...) -> None: ...
    @property
    def start(self): ...
    @property
    def epoch(self): ...
    @epoch.setter
    def epoch(self, epoch: Any) -> None: ...
    def __getitem__(self, item: Any): ...
    @property
    def count(self): ...
    def on_batch_end(self, batch: Any, logs: Any = ...) -> None: ...

def plot_weights(model: Any, dest_dir: Any, layers: Optional[Any] = ...) -> None: ...
