import praw
from enum import Enum
from typing import Any, List

class Category(Enum):
    CONROVERSIAL: str = ...
    GILDED: str = ...
    HOT: str = ...
    NEW: str = ...
    RISING: str = ...
    TOP: str = ...

class Scraper:
    _reddit: Any = ...
    def __init__(self) -> None: ...
    def scrape(self, subreddit_name: str=..., category: Category=..., num_submissions: int=...) -> List[praw.models.Submission]: ...

class RedditView:
    _display: Any = ...
    _scraper: Any = ...
    _polling_interval: Any = ...
    _subreddit: Any = ...
    _category: Any = ...
    def __init__(self, polling_interval: int=..., subreddit: str=..., category: Category=...) -> None: ...
    def _poll_titles(self, num: int) -> List[str]: ...
    def start_polling(self) -> None: ...
