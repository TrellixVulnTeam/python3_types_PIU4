import ev3
import task
from typing import Any

TRIAS: Any
ALLE_MEINE_ENTCHEN: Any
HAPPY_BIRTHDAY: Any

class Jukebox(ev3.EV3):
    _volume: int = ...
    _temperament: int = ...
    _pos_color: Any = ...
    _pos_tone: Any = ...
    _pos_led: Any = ...
    def __init__(self, protocol: str=..., host: str=..., ev3_obj: Any=...) -> None: ...
    @property
    def volume(self): ...
    @volume.setter
    def volume(self, value: int) -> Any: ...
    @property
    def temperament(self): ...
    @temperament.setter
    def temperament(self, value: float) -> Any: ...
    def change_color(self, led_pattern: bytes) -> None: ...
    def play_tone(self, tone: str, duration: float=...) -> None: ...
    def sound(self, path: str, duration: float=..., repeat: bool=...) -> task.Task: ...
    def stop(self) -> None: ...
    def _init_tone(self) -> None: ...
    def _next_tone(self, song: Any) -> float: ...
    def _init_color(self) -> None: ...
    def _next_color(self, song: Any) -> bool: ...
    def song(self, song: dict) -> task.Task: ...
